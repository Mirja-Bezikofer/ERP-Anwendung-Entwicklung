  as projection on zmd_r_urlaubsanspr
{
  key urlaubsanpr_id,
  key mitarbeiter, 
      jahr,
      urlaubstage,
    
      /* Administrative Data */
      CreatedBy,
      CreatedAt,
      LastChangedBy,
      LastChangedAt,

      /* Associations */
      _Employee : redirected to parent zmd_c_employee,   
}
