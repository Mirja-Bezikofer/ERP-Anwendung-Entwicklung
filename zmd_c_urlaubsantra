@EndUserText.label: 'PaidTimeOffAdministration'
@AccessControl.authorizationCheck: #NOT_REQUIRED
define root view entity zmd_c_urlaubsantra
  as projection on zmd_r_urlaubsatra
{
  key client,
  key urlaubsantrag_id,
  key antragssteller,
  key genehmigender,
      startdatum,
      enddatum,
      urlaubstage,
      kommentar,
      status,

  /* Administrative Data */
  CreatedBy,
  CreatedAt,
  LastChangedBy,
  LastChangedAt,

  /* Associations */
  _Employee : redirected to parent zmd_c_employee  /* Assoziation zu Employee */
}
